# Since the ".env" file is gitignored, you can use the ".env.example" file to
# build a new ".env" file when you clone the repo. Keep this file up-to-date
# when you add new variables to `.env`.

# This file will be committed to version control, so make sure not to have any
# secrets in it. If you are cloning this repo, create a copy of this file named
# ".env" and populate it with your secrets.

# When adding additional environment variables, the schema in "/src/env.js"
# should be updated accordingly.

# Next Auth
# You can generate a new secret on the command line with:
# npx auth secret
# https://next-auth.js.org/configuration/options#secret
AUTH_SECRET=""

# NextAuth URL (required for production)
NEXTAUTH_URL="https://your-domain.com"

# Next Auth Discord Provider
AUTH_DISCORD_ID=""
AUTH_DISCORD_SECRET=""

# Drizzle
DATABASE_URL=""

# SingleStore Database (alternative to DATABASE_URL)
SINGLESTORE_HOST=""
SINGLESTORE_USER=""
SINGLESTORE_PASS=""
SINGLESTORE_PORT="3306"


# Google Gemini API
GEMINI_API_KEY=""

# MTN Mobile Money API Configuration
MTN_MOMO_API_BASE_URL="https://sandbox.momodeveloper.mtn.com"
MTN_MOMO_PRIMARY_KEY=""
MTN_MOMO_SECONDARY_KEY=""
MTN_MOMO_API_USER_ID=""
MTN_MOMO_API_KEY=""
MTN_MOMO_ENVIRONMENT="sandbox"

# Orange Money API Configuration
ORANGE_MONEY_API_BASE_URL="https://api.orange.com/orange-money-webpay/dev/v1"
ORANGE_MONEY_AUTH_HEADER=""
ORANGE_MONEY_MERCHANT_KEY=""
